{
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": true,
    "source.organizeImports": true
  },
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "eslint.enable": false,
  "[firerules]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.formatOnSave": true
  },
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": false,
  "cSpell.words": [
    "cgext"
  ]
}
